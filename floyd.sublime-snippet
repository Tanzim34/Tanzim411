void floyd_warshall(int n)
{
	for(int k=1; k<=n; k++)
		for(int i=1; i<=n; i++)
			for(int j=1; j<=n; j++)
				d[i][j] = Min(d[i][j],d[i][k]+d[k][j]);
}
]]></content>
